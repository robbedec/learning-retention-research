@Article{DzulkifliMustafar2012,
  author   = {Dzulkifli, Mariam Adawiah and Mustafar, Muhammad Faiz},
  title    = {The Influence of Colour on Memory Performance: A Review},
  journal  = {The Malaysian journal of medical sciences},
  year     = {2012},
  volume   = {20},
  pages    = {3-9},
  abstract = { Human cognition involves many mental processes that are highly interrelated, such as perception, attention, memory, and thinking. An important and core cognitive process is memory,
	which is commonly associated with the storing and remembering of environmental information. An interesting issue in memory research is on ways to enhance memory performance, and thus,
	remembering of information. Can colour result in improved memory abilities? The present paper highlights the relationship between colours, attention, and memory performance. The significance
	of colour in different settings is presented first, followed by a description on the nature of human memory. The role of attention and emotional arousal on memory performance is discussed next.
	The review of several studies on colours and memory are meant to explain some empirical works done in the area and related issues that arise from such studies.},
  comment  = {Hier wordt de positieve impact van kleur op het onthouden van informatie besproken. Er zijn drie soorten geheugen: het sensorisch register, korte termijn en lange termijn. In het sensorisch register wordt heel veel data opgeslagen voor een heel korte periode. Deze informatie is data dat men verzameld uit de directe omgeving. Om informatie van het sensorisch register geheugen naar het korte termijn te verplaatsen, moet men een bepaalde hoeveelheid aandacht besteden aan die informatie. Hoe meer aandacht er aan wordt besteed, hoe sneller het in het korte termijn geheugen zit. Dit is onder meer een van de redenen waarom multitasking niet werkt, de aandacht wordt verdeeld onder meerdere bronnen van informatie. Er wordt aangetoond dat kleur de aandacht kan versterken en dus zorgt voor beter opslaan van de informatie. Ook worden kleuren vaak met bepaalde gevoelens gekoppeld, bijvoorbeeld groen en kalmte. De gevoelens bij die kleuren hebben dan weer een effect op de aandacht. Uit de gerefereerde onderzoeken blijkt dat kleur een positieve impact heeft op het onthouden van informatie. Alhoewel we het exacte mechanisme van informatie opslaan hiermee niet duidelijk is, blijkt er wel een verband te zijn tussen het opslaan van informatie en de methode waarmee die informatie aangereikt wordt.},
  keywords = {attention, colour, long term, mental recall, short term},
}

@Article{GaisEtAl2005,
  author    = {Gais, Steffen and Lucas, Brian and Born, Jan},
  title     = {Sleep after learning aids memory recall},
  journal   = {Learning \& Memory},
  year      = {2005},
  volume    = {13},
  pages     = {259-262},
  doi       = {10.1101/lm.132106},
  abstract  = {In recent years, the effect of sleep on memory consolidation has received considerable attention. In humans, these studies concentrated mainly on procedural types of memory, which are considered to be hippocampus-independent. Here, we show that sleep also has a persisting effect on hippocampus-dependent declarative memory. In two experiments, we examined high school students’ ability to remember vocabulary. We show that declarative memory is enhanced when sleep follows within a few hours of learning, independent of time of day, and with equal amounts of interference during retention intervals. Sleep deprivation has a detrimental effect on memory, which was significant after a night of recovery sleep. Thus, fatigue accumulating during wake intervals could be ruled out as a confound.},
  comment   = {Hier toont ook weer aan dat vermoeidheid wel degelijk een impact heeft op het onthouden van leerstof. Ook toont deze studie aan dat slapen na het leren zorgt voor betere opname van informatie.},
  publisher = {Cold Spring Harbor Laboratory Press},
}

@Article{Darolia2013,
  author   = {Darolia, Rajeev},
  title    = {Working (and studying) day and night: Heterogeneous effects of working on the academic performance of full-time and part-time students},
  journal  = {Economics of Education Review},
  year     = {2013},
  volume   = {38},
  pages    = {38-50},
  doi      = {https://doi.org/10.1016/j.econedurev.2013.10.004},
  abstract = {A growing number of students are working while in college and to a greater extent. Using nationally representative data from the 1997 National Longitudinal Survey of Youth, I analyze the effect of working on grades and credit completion for undergraduate students in the United States. Strategies to identify the causal relationship between working and academic performance include student-level fixed effects to control for permanent,
	unobserved characteristics that may affect both work and study intensity, and system GMM models to account for potentially endogenous relationships between working and academic performance that vary over time. I examine the consequences of working for heterogeneous subgroups, with a particular focus on differences between full-time and part-time students. I find no evidence that students’ grades are harmed by marginal work hours, but that full-time students complete fewer credits per term when increasing work},
  comment  = {Hier heeft een onderzoek gedaan naar wat het effect is van werken tijdens het studeren voor vol- en deeltijdse studenten. Uit deze studie blijkt dat voor deeltijdse studenten dit kan leiden tot minder krediet op te nemen per semester, maar dit geen impact heeft op het al dan niet verwerven van het opgenomen krediet. Terwijl voor voltijdse studenten werken tijdens het studeren zorgt voor een lagere kredietscore op het einde van het semester. Dit kan onder andere komen door het vervangen van les gerelateerde activiteiten met werk.},
  keywords = {Higher education, Hours of work, Part-time students, Educational finance},
}

