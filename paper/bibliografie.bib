@Article{BurkeH.Bretzing1979,
  author    = {Burke H. Bretzing, Raymond W. Kulhavy},
  title     = {Notetaking and depth of processing},
  journal   = {Contemporary Educational Psychology},
  year      = {1979},
  volume    = {4},
  number    = {2},
  month     = {4},
  pages     = {145-153},
  doi       = {10.1016/0361-476x(79)90069-9},
  abstract  = {Four levels of notetaking (summary, paraphrase, verbatim, and letter search) were used to control depth of processing of a prose passage with 180 high school students, who then either reviewed their notes or read an interpolated text. A separate control group took no notes. On immediate and delayed post-tests, post hoc analyses with the depth (notetaking) condition showed the following ranking: summary = paraphrase > control = verbatim > letter search. A paraphrase notes × review × test-position interaction was significant, indicating that less forgetting occurred on a delayed post-test when students reviewed their paraphrase notes than when they read an interpolated text. Analysis of reading times showed that the additional time required for notetaking was only worthwhile when meaningful notes were taken.},
  comment   = {In dit artikel wordt nagegaan wat het effect van vier methodes van nota nemen is voor het nemen van een test.
De vier methodes zijn samenvatten, parafraseren, woordelijk noteren en het zoeken van letters.
Ook wordt nagegaan wat het effect is op het lezen van de nota's voor de test.
De vier methodes kunnen volgens dit artikel aldus worden geranschikt:
Samenvatten is ongeveer even effectief als parafraseren maar beter dan alles herschrijven.
Herschrijven heeft geen significant verschil t.o.v. de controle.
Het zoeken van letters zorgt ervoor dat de testnemer de tekst oppervlakkiger leest en heeft dus een negatief effect op het resultaat.},
  keywords  = {note taking,summary,paraphrasing,verbatim,letter search,review of notes},
  publisher = {Elsevier},
}

