@Article{McDanielEtAl2011,
  author       = {Mark A. McDaniel and Pooja K. Agarwal and Barbie J. Huelser and Kathleen B. McDermott and Henry L. Roediger},
  title        = {Test-Enhanced Learning in a Middle School Science Classroom: The Effects of Quiz Frequency and Placement},
  journaltitle = {Journal of Educational Psychology},
  year         = {2011},
  month        = feb,
  abstract     = {When three quizzes on the content were spaced across the coverage of a unit, effects were robust, producing between 13\%and 25\% gains in performance on unit exams across a range of eighth-grade science topics, including genetics, evolution, anatomy, astronomy, and chemistry. Perhaps more telling, the proportion of otherwise unlearned material (i.e., material with incorrect answers for non-quizzed items) that benefited from quizzing was substantial, ranging from .61 (Experiment 2a) to .78 (Experiment2b, for the three-quizzed conditions). From the standpoint of projected grades, the benefits of quizzing were equally impressive. Performance levels on material that was not quizzed were at the C level for the school’s grading scale quizzing generally in-creased performance levels on the material to an A– level. This Isa robust effect, especially considering that the science teacher for this grade level provided conscientious, rich instruction for her students, with the classroom experience involving active learning exercises and demonstrations. The summative exams reflected the content emphasized in the textbook and the classroom activities. Even with these highly favorable pedagogical factors in place, quizzing improved learning and exam performance for the course content relative to no quizzing. Further, the present quizzing effects might slightly underestimate the true effect because quizzing can augment students’ performance on related content, and to the extent that in some cases random assignment distributed one of each into quiz and nonquiz conditions, then there would be a chance for some carryover effects to nonquizzed items. If this happened, the direct effect of quizzing would be underestimated because the control comparison would be too high (due to the spillover effect of quizzing on related items).},
  file         = {:artikels/Test-Enhanced Learning in a Middle School Science Classroom The Effects of Quiz Frequency and Placement.pdf:PDF},
}

@Article{Landauer1986,
  author   = {Thomas K. Landauer},
  title    = {How Much Do People Remember Some Estimates of the Quantity of Learned Information in Long-term Memory},
  year     = {1986},
  abstract = {How much information from experience does a normal adult remember? The “functional information content” of human memory was estimated in several ways. The methods depend on measured rotes of input and loss from very long-term memory and on analyses of the informational demands of human memory based performance. Estimates ranged around 10’ bits. It is speculated that the flexible and creative retrieval of facts by humans is a function of a large ratio of “hardware” capacity to functional storage requirements.
Several different methods for estimating the approximate functional content of adult human memory have been tried. Estimates were obtained of the rate at which information is entered into, and lost from, long-term memory. These estimates were then used in three ways to arrive at total capacity estimates. One way assumed that information is accumulated linearly throughout life, yielding an estimate of 1.5 x 109 bits for a 35-year-old person. Second, the estimate was made more realistic by taking into account loss rates, reducing the amount of retrievable information held by midlife to around 109 bits. Third, the question of how much knowledge an adult needs to support knowledge-based behavior was considered. A guess of about 0.5 x 109, based largely on estimates derived from word knowledge, was hazarded. The estimates all point toward a functional learned memory content of around a billion bits for a mature person.},
  file     = {:artikels/How Much Do People Remember! Some Estimates of the Quantity of Learned Information in Long-term Memory.pdf:PDF},
}

@Article{HollingworthEtAl2001,
  author       = {Andrew Hollingworth and Carrick C. Williams and John M. Henderson},
  title        = {To see and remember: Visually specificinformation is retained in memory frompreviously attended objects in natural scenes},
  journaltitle = {Psychonomic Bulletin \& Review},
  year         = {2001},
  abstract     = {The visual perception of natural scenes is a dynamic process during which local objects are selected by movements of the eyes and attention. The selective nature of scene perception, however, does not necessarily entail that visual representation is limited to the currently selected object. The results of this study demonstrate that visual object representations are retained in memory from previously attended regions of a scene. When attention is withdrawn from an object, there are indeed representational consequences of having attended that object. Thus, the present study supports a view of scene perception in which memory and perceptual processing are functionally integrated.},
  file         = {:artikels/Hollingworth2001_Article_ToSeeAndRememberVisuallySpecif.pdf:PDF},
}

@Article{MokHilgerink2017,
  author   = {Samuel Mok and Tom Hilgerink},
  title    = {Gebruikte studiemethodes en hun effectiviteit: een beschouwing},
  year     = {2017},
  abstract = {In een literatuurstudie bekijkt het onderzoek wat de effecten zijn van verschillende studiemethodes op resultaten. Vervolgens onderzoekt ment aan de hand van gesloten vragen hoe de testpersonen leren voor een test. Uit het vergelijken van de resultaten met de literatuurstudie kan men twee groepen onderscheiden: sterke en zwakke studenten. Sterke studenten gebruiken effectieve studiemethodes zoals meer studeren dan gemiddeld, spreidend studeren, huiswerk, enz. en halen hierdoor betere resultaten dan gemiddeld. Zwakke studenten leggen de focus op minder effectieve studiemethodes zoals hoorcolleges volgen, niet op tijd beginnen met leren, niet maken van opgaves, enz. en halen hierbij slechtere resultaten dan gemiddeld. In de open vragen kan men ook concluderen dat studenten digitaal studiemateriaal essentieel vinden. 
},
  file     = {:artikels/Artikel 1.pdf:PDF},
}

@Article{Nijhof2005,
  author   = {Michelle Nijhof},
  title    = {De Studiewijzer als instrument om studenten inzicht te bieden in hun studiemethode},
  year     = {2005},
  abstract = {Men heeft een website ontwikkeld waarmee studenten zich kunnen laten informeren, testen en adviseren op gebied van een vijftal studiefactoren: leerstijlen, studievaardigheden, studiemotieven, studieopvattingen en studieproblemen. Aan de hand van een vragenlijst wordt er onderzoek gedaan of de website helpt bij het verbeteren of veranderen van de huidige studiemethode van de testpersonen. Uit de resultaten blijkt dat de testpersonen de website positief beoordelen en ook nood hebben aan dergelijke hulpmiddelen. In een volgend onderzoek zou men kunnen kijken of de website effectief verbetering brengt in de studieresultaten van de testpersonen. 

Het onderzoek bevat wel enkele opmerkingen die niet onbesproken kunnen worden. Als eerste opmerking deden er slechts 17 testpersonen mee aan de vragenlijst. De resultaten zijn dus niet volledig betrouwbaar. De opgestelde grafieken voldoen ook niet aan de ‘best-practices’ van wetenschappelijk onderzoek. Men maakt gebruik van een taartdiagram, kleurovergang als achtergrond en 3D staafdiagrammen.
},
  file     = {:artikels/Artikel 2.pdf:PDF},
}

@Article{PulsEtAl2012,
  author   = {Jurjen Puls and Ine Verstappen and Leo Wagemans},
  title    = {Doelgericht studeren},
  year     = {2012},
  abstract = {Doelgericht studeren is opgebouwd in een aantal belangrijke stappen. Zo zijn er al enkele stappen die je moet doornemen voordat je weldegelijk begint met studeren. Studeren doe je best in een rustige omgeving met zo weinig mogelijk afleiding. Je stelt voor jezelf concrete doelen en maakt hieruit een haalbare planning. Deze is best opgebouwd uit een afwisseling van studietijd en vrije tijd, zo houd je beter vol. Pauzes zijn belangrijk omdat je het meeste leert aan het begin en aan het einde van een studiesessie. Een laatste belangrijke stap voor je begint met studeren, is het overlopen van de structuur en opbouw van de leerstof a.d.h.v. de inhoudsopgave. In het artikel wordt niks gezegd over de meest efficiënte manier van studeren, ze geven enkel een aantal manieren om te studeren. Als je studeert door te onderstrepen lees je de tekst beter eerst globaal, en onderstreep je pas bij een tweede keer lezen, maar deze keer intensief lezen. Over aantekeningen maken, samenvattingen maken en schema’s maken werd ook gesproken, maar hiervoor moet je alsook intensief lezen om de hoofdzaken van de bijzaken te kunnen onderscheiden. Er werd ook duidelijk verteld dat je best na iedere studiesessie jezelf toetst op wat je zonet studeerde. Als je dan merkt dat je enkele onderdelen minder goed kent, kan je daarna bij het herhalen je focus daarop leggen. },
  file     = {:artikels/Artikel 4.pdf:PDF},
}

@Article{Pelgrim2011,
  author       = {Corina Pelgrim},
  title        = {De invloed van samenwerkend leren in het leesstrategieonderwijs op de motivatie en de resultaten voor leesvaardigheid voor het vak Frans},
  journaltitle = {Levende Talen Tijdschrift, jaargang 12, nummer 3},
  year         = {2011},
  abstract     = {In dit onderzoek onderzocht men of leren in groepsverband een positief effect zou hebben op de eindresultaten. Er werd gebruik gemaakt van twee kleine groepen studenten die moesten samenwerken in verschillende groepjes. Hierbij varieerde de grootte en methode van samenwerken regelmatig. De nulmeting werd genomen aan het begin van het schooljaar toen de studenten nog geen enkele les hadden gevolgd, de eindexamens vormden de eindmeting. Het gemiddelde bij de eindmeting lag hoger, maar niet hoog genoeg om van een werkelijke verbetering door samenwerkend leren te spreken. Ook was deze eindmeting gepaard met een hoge deviatie. De finale conclusie is dat er geen effect was op de korte termijn van het onderzoek en dat men een langer onderzoek zou moeten voeren om effectieve resultaten te bekomen, maar er was wel een duidelijke toename in motivatie en voldoening wat er op zou wijzen dat de leerlingen zich bewuster en verantwoordelijker voelden over hun leerproces.},
  file         = {:artikels/De invloed van samenwerkend leren in het leesstrategieonderwijs op de motivatie en de resultaten voor leesvaardigheid voor het vak Frans.pdf:PDF},
}

@Article{TzeChou2010,
  author       = {Peter Tze and Ming Chou},
  title        = {Attention drainage effect: How background music effects concentration in Taiwanese college students},
  journaltitle = {Journal of the Scholarship of Teaching and Learning, Vol. 10, No. 1},
  year         = {2010},
  abstract     = {In deze studie werd er onderzocht of achtergrondmuziek kan leiden tot concentratieverlies tijdens een leestest of juist voor betere resultaten zorgt. In hun onderzoek werd er gebruik gemaakt van klassieke muziek en hip hop. 113 studenten uit Taiwan werden onderverdeeld in 3 groepen en moesten allen dezelfde test maken. De studenten waren allen 2de jaars studenten uit dezelfde school en allen hadden ze al enige kennis over het onderwerp waarop ze getoetst werden. Hun leeftijd varieerde van 20 tot 50 jaar oud. De eerste groep moest naar klassieke muziek luisteren tijdens het afleggen van de test, de tweede groep naar hip hop en de derde groep kreeg geen muziek te horen. De groep zonder muziek had de beste resultaten, maar het verschil met de groep die klassieke muziek luisterden was niet groot genoeg om de achtergrondmuziek als oorzaak aan te duiden. Bij de hip hop groep lagen de scores het laagst en bij deze groep was het duidelijk dat dit door de invloed van de muziek was. },
  file         = {:artikels/Attention drainage effect How background music effects concentration in Taiwanese college students.pdf:PDF},
}

@Online{DochyEtAl2018,
  author   = {Wibran Dochy and Filip Dochy and Margo Janssens},
  title    = {Assessment as Learning (AaL) - Bouwsteen van High Impact Learning that Lasts (HILL)},
  year     = {2018},
  url      = {http://www.highimpactlearningthatlasts.com/Documenten/AssessmentAsLearning_NL.pdf},
  abstract = {Assessment as Learning is een aanpak die de verziekte ‘testcultuur’ wilt aanpakken. Het steunt hierbij op het HILL-model. Een model dat bestaat uit zeven bouwstenen die een beduidend blijvende impact hebben op het leerproces. AaL staat haaks op traditionele leerprogramma’s die te veel gebruik maken van summatieve toetsen. De focus ligt op hoe de leerling omgaat met de problemen en hoe hij betekenis geeft aan verschillende structuren en concepten. De student moet kunnen aantonen dat hij bronnen en instrumenten in het probleemoplossingsproces goed kan beheren. Hierbij wordt hij bijgestaan door een expert of ‘coach’. Kennis ontstaat in interactie met anderen en ‘learner agency’ te kunnen uitoefenen. Het is belangrijk om collaboratieve activiteiten op zodanige wijze te ondersteunen, dat de student deze kan uitvoeren terwijl hij gecoacht wordt door anderen. De coach is expert, soms instructor maar vaak een ‘peer’. Dit houdt in dat docenten of coaches mee uitzoeken, analyseren en reflecteren over de informatie die uit dialoog naar boven komt. Studenten worden beoordeeld door de evolutie in het leerproces, hoe ze omgaan met feedback en hoe ze problemen oplossen.},
  file     = {:artikels/Artikel 5.pdf:PDF},
}

@Article{WeidmanBaker2015,
  author       = {Joseph Weidman and Keith Baker},
  title        = {The Cognitive Science of Learning: Concepts and Strategies for the Educator and Learner},
  journaltitle = {Anesthesia \& Analgesia, volume 121, number 6},
  year         = {2015},
  abstract     = {Deze studie focust op wat zowel leraar als leerling kunnen doen om beter te leren of beter leerstof aan te brengen. Het artikel volgt een zeer technische aanpak en baseert zich op de cognitieve wetenschap 

Leraar

Studie toont aan dat leerstof die wordt aangebracht a.d.h.v. de cognitive load theory en enorme verbetering biedt tegenover traditionele studiemethoden. De theorie houdt rekening met de limieten van het menselijk brein en zorgt ervoor dat er geen overload aan informatie gegeven wordt want die zetten een rem op het verwerken van nieuwe informatie. Een andere term die vaak terugkomt in het artikel is het constructivisme. Wanneer nieuwe leerstof gegeven wordt is het belangrijk dat de leraar eerst de vooroordelen van de student leert begrijpen en die al dan niet weerlegt indien ze fout zijn. Zo ontstaat er geen verwarring wanneer nieuwe informatie gegeven wordt over bestaande concepten.

Leerling

Het artikel legt nadruk op het belang van metacognitie. Metacognitie is de kennis over de eigen kennis. Het is een manier waarop je reflecteert over je eigen kennis van een bepaald onderwerp. Studie toont aan dat mensen die vaker aan metacognitie doen efficiënter kunnen leren. In het artikel wordt ook de vergelijking gemaakt tussen retrieval en traditionele leermethoden. De retrieval-methode bestaat eruit de geleerde informatie herhaaldelijk op te vissen uit het geheugen om zo de leerstof onder te knie te krijgen. De laatste methode die wordt aangehaald is de spaced learning methode. Deze methode verdeelt het aanleren van studiemateriaal op in een aantal perioden. Tussen de periode zit genoeg tijd zodat de leerlingen de tijd krijgen om te stil te staan bij de opgedane kennis. Deze methode staat tegenover de methode waar alle informatie in één periode in de hersenen gepropt wordt. 

Het artikel maakt gebruik van Cohen’s D (effectgrootte) om het verschil aan te tonen tussen de verschillende methoden. 
},
  file         = {:artikels/CognitiveScienceOf Learning-ConceptsAndStrategiesForTheEducatoR AndLearner.PDF:PDF},
}

